{
  "name": "fanger",
  "version": "0.2.8",
  "description": "Defang and refang IoC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier src/**/*.ts",
    "prettier:write": "prettier --write src/**/*.ts",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "bin": {
    "defang": "dist/bin/defang.js",
    "refang": "dist/bin/refang.js"
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint:fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ninoseki/fanger.git"
  },
  "author": "Manabu Niseki",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ninoseki/fanger/issues"
  },
  "homepage": "https://github.com/ninoseki/fanger#readme",
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.2",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^6.0.0",
    "jest": "^27.0.4",
    "lint-staged": "^11.0.0",
    "pinst": "^2.1.6",
    "prettier": "^2.3.1",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "escape-string-regexp": "4.0.0",
    "get-stdin": "8.0.0",
    "ioc-extractor": "2.7.8"
  }
}
